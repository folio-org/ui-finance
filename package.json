{
  "name": "@folio/finance",
  "version": "1.4.1",
  "description": "Description for ui-finance",
  "main": "src/index.js",
  "repository": "",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.4"
  },
  "stripes": {
    "type": "app",
    "displayName": "ui-finance.meta.title",
    "moduleName": "Finance",
    "route": "/finance",
    "home": "/finance",
    "hasSettings": true,
    "okapiInterfaces": {
      "budget": "1.0",
      "fiscal_year": "1.0",
      "fund": "1.0",
      "ledger": "1.0"
    },
    "queryResource": "query",
    "icons": [
      {
        "name": "app",
        "alt": "Create, view and manage Finance",
        "title": "Finance"
      }
    ],
    "permissionSets": [
      {
        "permissionName": "module.finance.enabled",
        "displayName": "UI: Finance module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.finance.enabled",
        "displayName": "Settings (Finance): Display list of settings for Finance",
        "visible": true,
        "subPermissions": [
          "settings.enabled"
        ]
      },
      {
        "permissionName": "ui-finance.view",
        "displayName": "Finance: View ledgers, funds, budgets and fiscal years",
        "description": "",
        "visible": true,
        "subPermissions": [
          "budget.collection.get",
          "budget.item.get",
          "fiscal_year.collection.get",
          "fiscal_year.item.get",
          "fund.collection.get",
          "fund.item.get",
          "ledger.collection.get",
          "ledger.item.get"
        ]
      },
      {
        "permissionName": "ui-finance.all",
        "displayName": "Finance: all permissions for View and Edit ledgers, funds, budgets and fiscal years",
        "description": "",
        "visible": true,
        "subPermissions": [
          "budget.all",
          "fiscal_year.all",
          "fund.all",
          "ledger.all"
        ]
      }
    ]
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test nightmare --run demo --show",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@folio/eslint-config-stripes": "^4.0.1",
    "@folio/stripes": "^2.0.0",
    "@folio/stripes-cli": "^1.4.0",
    "babel-eslint": "^9.0.0",
    "react-intl": "^2.4.0",
    "eslint": "^5.6.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "query-string": "^6.1.0",
    "react-router-dom": "^4.1.1",
    "redux-form": "^7.3.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^2.0.0",
    "react": "*"
  }
}
