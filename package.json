{
  "name": "@folio/finance",
  "version": "1.6.0",
  "description": "Description for ui-finance",
  "main": "src/index.js",
  "repository": "",
  "publishConfig": {
    "registry": "https://repository.folio.org/repository/npm-folio/"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.9.4"
  },
  "stripes": {
    "actsAs": ["app", "settings"],
    "displayName": "ui-finance.meta.title",
    "moduleName": "Finance",
    "route": "/finance",
    "home": "/finance",
    "okapiInterfaces": {
      "configuration": "2.0",
      "finance-storage.budgets": "1.0",
      "finance-storage.fiscal-years": "1.0",
      "finance-storage.funds": "2.0",
      "finance-storage.ledgers": "1.0"
    },
    "queryResource": "query",
    "icons": [
      {
        "name": "app",
        "alt": "Create, view and manage Finance",
        "title": "Finance"
      }
    ],
    "permissionSets": [
      {
        "permissionName": "module.finance.enabled",
        "displayName": "UI: Finance module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.finance.enabled",
        "displayName": "Settings (Finance): Display list of settings for Finance",
        "visible": true,
        "subPermissions": [
          "settings.enabled"
        ]
      },
      {
        "permissionName": "ui-finance.third-party-services",
        "displayName": "Finance: Permissions required to call services apart from mod-finance",
        "description": "",
        "subPermissions": [
          "configuration.entries.collection.get"
        ]
      },
      {
        "permissionName": "ui-finance.view",
        "displayName": "Finance: View ledgers, funds, budgets and fiscal years",
        "description": "",
        "visible": true,
        "subPermissions": [
          "ui-finance.third-party-services",
          "finance-storage.budgets.collection.get",
          "finance-storage.budgets.item.get",
          "finance-storage.fiscal-years.collection.get",
          "finance-storage.fiscal-years.item.get",
          "finance-storage.funds.collection.get",
          "finance-storage.funds.item.get",
          "finance-storage.ledgers.collection.get",
          "finance-storage.ledgers.item.get"
        ]
      },
      {
        "permissionName": "ui-finance.all",
        "displayName": "Finance: all permissions for View and Edit ledgers, funds, budgets and fiscal years",
        "description": "",
        "visible": true,
        "subPermissions": [
          "ui-finance.third-party-services",
          "finance-storage.budgets.all",
          "finance-storage.fiscal-years.all",
          "finance-storage.funds.all",
          "finance-storage.ledgers.all"
        ]
      }
    ]
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "test": "stripes test karma",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@bigtest/interactor": "^0.9.0",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^4.0.1",
    "@folio/stripes": "^2.7.0",
    "@folio/stripes-cli": "^1.8.0",
    "babel-eslint": "^9.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "react-intl": "^2.4.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-redux": "^5.0.7",
    "react-router-prop-types": "^1.0.4",
    "redux": "^4.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "query-string": "^6.1.0",
    "react-router-dom": "^4.1.1",
    "redux-form": "^7.3.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^2.7.0",
    "react": "*"
  }
}
